{
  "name": "@lykmapipo/logger",
  "version": "0.5.15",
  "description": "Naive logging helpers",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "clean": "rimraf lib dist es umd logs",
    "prebuild": "npm test && npm run clean",
    "build": "rollup -c",
    "lint": "eslint --fix --ext .js src/ test/ rollup.config.js",
    "pretest": "npm run lint",
    "posttest": "rimraf logs",
    "test": "NODE_ENV=test mocha --require @babel/register test/**/*.spec.js",
    "docs": "doxdox 'lib/**/*.js' -p package.json -l markdown -o DOCUMENTATION.md",
    "cmt": "git add -A && git-cz",
    "changelog": "changelog"
  },
  "keywords": [
    "lykmapipo",
    "logger",
    "winston",
    "logfmt",
    "utils",
    "helpers"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lykmapipo/logger.git"
  },
  "author": {
    "name": "lykmapipo",
    "email": "lallyelias87@gmail.com",
    "url": "https://github.com/lykmapipo"
  },
  "contributors": [
    {
      "name": "lykmapipo",
      "email": "lallyelias87@gmail.com",
      "url": "https://github.com/lykmapipo"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lykmapipo/logger/issues"
  },
  "homepage": "https://github.com/lykmapipo/logger#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.1",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/register": "^7.10.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/travis-cli": "^8.3.5",
    "chai": "^4.2.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "doxdox": "^3.0.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-mocha": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "generate-changelog": "^1.8.0",
    "handlebars": "^4.7.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "mocha": "^7.2.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.11.2",
    "shelljs": "^0.8.4",
    "std-mocks": "^1.0.1"
  },
  "peerDependencies": {},
  "dependencies": {
    "@lykmapipo/common": ">=0.34.2",
    "@lykmapipo/env": ">=0.17.7",
    "lodash": ">=4.17.15",
    "winston": ">=3.2.1",
    "winston-daily-rotate-file": ">=4.4.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint",
      "git add -A"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
